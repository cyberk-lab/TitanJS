{
  "lerna": "6",
  "conventionalCommits": true,
  "npmClient": "yarn",
  "npmClientArgs": ["--no-lockfile"],
  "packages": ["packages/*", "networks/*", "libs/*"],
  "version": "0.2.4",
  "registry": "https://registry.npmjs.org",
  "useWorkspaces": true,
  "command": {
    "create": {
      "homepage": "https://github.com/cyberk-lab/titanlabjs",
      "license": "SEE LICENSE IN LICENSE",
      "access": "restricted"
    },
    "publish": {
      "allowBranch": "first-publish",
      "message": "chore(release): publish"
    }
  }
}
