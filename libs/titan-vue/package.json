{
  "name": "@titanlabjs/titan-vue",
  "version": "0.2.1",
  "description": "titan-vue is a JavaScript library providing vue hooks for interacting with Cosmos SDK based blockchains.",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/cyberk-lab/titanjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyberk-lab/titanjs"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE-MIT ../../LICENSE-Apache README.md package.json dist",
    "clean": "rimraf dist/**",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy",
    "build:dev": "npm run clean; tsc --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.1",
    "@titanlabjs/cosmos": "^0.2.1",
    "@titanlabjs/cosmos-types": "^0.2.1",
    "@titanlabjs/encoding": "^0.2.1",
    "@titanlabjs/pubkey": "^0.2.1",
    "@titanlabjs/types": "0.0.2",
    "@titanlabjs/utils": "^0.2.1",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@tanstack/vue-query": "^5.62.7",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "@tanstack/vue-query": "^5.62.7",
    "vue": "^3.5.13"
  },
  "keywords": [
    "cosmos",
    "blockchain",
    "transaction",
    "cosmjs",
    "wallet"
  ]
}
